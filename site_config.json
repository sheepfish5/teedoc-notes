{
    "site_name": "sheepfish5-notes",
    "site_slogon": "PHP是世界上最好的语言! ",
    "site_root_url": "/teedoc-notes/",
    "site_domain": "bethesda.asia",
    "site_protocol": "https",
    "config_template_dir": "config",
    "route": {
        "docs": {
            "/多媒体处理/": "docs/多媒体处理",
            "/前端/": "docs/前端",
            "/香橙派/": "docs/香橙派",
            "/C++/": "docs/C++",
            "/Docker/": "docs/Docker",
            "/Git/": "docs/Git",
            "/Java/": "docs/Java",
            "/JavaScript/": "docs/JavaScript",
            "/Linux/": "docs/Linux",
            "/Vue/": "docs/Vue"
        },
        "pages": {
            "/": "pages/index"
        },
        "assets": {
            "/images/": "docs/images",
            "/static/": "static"
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "show_print_page": false,
                "env":{
                    "main_color": "#4caf7d"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#4caf7d",
                    "second_color": "#0a7d43"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}
